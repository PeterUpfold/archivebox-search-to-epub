{
    "archivebox_root": "/some/folder/ArchiveBox/data/archive",
    "localpath_replace": "archive/",
    "output_dir": "/some/output/folder",
    "invoke_node_cmdline": "node",
    "embed_images_tool": "/some/tools/folder/embed-images-as-data-uris/embed-images-as-data-uris.js",
    "ebook_convert_cmdline": "ebook-convert",
    "max_image_size": "700x500",
    "output_profile": "sony300",
    "archivebox_add_args":
        [
            "podman",
            "run",
            "--rm",
            "-i",
            "--user",
            "USERID",
            "--volume",
            "/some/volume/data:/data",
            "localhost/archivebox-on-podman",
            "add"
        ]
}